pipeline {
    agent any
    
    stages{
        stage('CheckOut')
        {
            steps
            {
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'gitrahul', url: 'https://github.com/devrahul19/flask-jenkin.git']])
            }
        }
        stage('Build')
        {
            steps
            {
                echo 'building'
             // sh 'sudo make build'
            }
        }
        stage('Run')
        {
            steps
            {    
                 echo 'Running'
             // sh 'sudo make run'
            }
        }
        stage('Test')
        {
            steps
            {    echo 'Testing'
             //   sh 'sudo make test'
            }
        }
        stage('Analysing')
        {
            steps
            {    echo 'Testing'
             //   sh 'sudo make test'
            }
        }
        stage('Deploying')
        {
            steps
            {    echo 'Testing'
             //   sh 'sudo make test'
            }
        }
        stage('Releasing')
        {
            steps
            {    echo 'Testing'
             //   sh 'sudo make test'
            }
        }
        stage('Monitoring')
        {
            steps
            {    echo 'Testing'
             //   sh 'sudo make test'
            }
        }
    }
}
